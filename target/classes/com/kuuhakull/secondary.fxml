<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>

<VBox
        stylesheets="@style.css" alignment="CENTER" spacing="20.0"
        xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1"
        fx:controller="com.kuuhakull.SecondaryController"
        minHeight="500.0" minWidth="800.0">
    <children>
        <Button fx:id="Button0" text="Пройти тест" onAction="#startTest" />
        <Button fx:id="Button1" text="Кейсовые задачи" onAction="#startCase" />
        <Button fx:id="Button2" text="Вернуться назад" onAction="#switchToPrimary" />
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
